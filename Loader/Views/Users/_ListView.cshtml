@model Loader.Models.ApplicationUser
@{
    var test = Model;
    Loader.Service.UsersService usrObj = new Loader.Service.UsersService();
}




<tr class="detailsList">
    <td class="listDrill" id="@Model.Id" style="vertical-align:top;">
        @*<a class="inner listDrill" User-caption="@Model.UserName" style="cursor:pointer;"> @Model.UserName</a>*@
        @Model.UserName
    </td>
    <td>
        @Model.Email
    </td>
    <td>
        @Model.IsUnlimited
    </td>
    <td>
       @usrObj.GetMenuTemplateName(Model.MTId)
    </td>

   
@if (Model.UserName != "super.admin")
{
    <td><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="btn btn-primary btn-xs editList" id="@Model.Id" data-title="Edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>

    @*<td><p data-placement="top" data-toggle="tooltip"  title="Delete"><button disabled class="btn btn-danger btn-xs deleteNode" id="@Model.Id" data-title="Delete"><span class="glyphicon glyphicon-trash"></span></button></p></td>*@
}
</tr>



