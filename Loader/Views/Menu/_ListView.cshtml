@model Loader.Models.Menu
@{
    var test = Model;
}




<tr class="detailsList">
    <td class="listDrill" id="@Model.MenuId" style="vertical-align:top;"  >
        <a class="inner listDrill" menu-caption="@Model.MenuCaption" style="cursor:pointer;"> @Model.MenuCaption</a>
    </td>
    <td>
        @Model.IsGroup
    </td>
    <td>
        @Model.IsContextMenu
    </td>
    <td>
        @Model.IsEnable
    </td>
 

    <td>
        @*<input type="button" id="@Model.MenuId" class="btn btn-link editList" value="Edit" />
            <input type="button" id="@Model.MenuId" class="btn btn-link deleteNode" value="Delete" />*@

    <td><p data-placement="top" data-toggle="tooltip" title="Edit"><button class="btn btn-primary btn-xs editList" id="@Model.MenuId"data-title="Edit"><span class="glyphicon glyphicon-pencil"></span></button></p></td>
    <td><p data-placement="top" data-toggle="tooltip" title="Delete"><button class="btn btn-danger btn-xs deleteNode" id="@Model.MenuId" data-title="Delete" ><span class="glyphicon glyphicon-trash"></span></button></p></td>

</tr>
